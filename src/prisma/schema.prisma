generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id          String  @id @default(uuid())
  name        String
  email       String
  avatar      String?
  password    String
  address     String
  city        String
  houseNumber String
  phoneNumber String
}

model Food {
  id          String @id @default(uuid())
  name        String
  description String
  price       Int
}
