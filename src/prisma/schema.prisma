generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id           String  @id @default(uuid())
  name         String
  email        String  @unique
  avatar       String?
  password     String
  address      String
  city         String
  house_number String
  phone_number String
}

model Food {
  id          String @id @default(uuid())
  name        String
  description String
  price       Int
}
